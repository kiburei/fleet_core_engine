<% content_for :header do %>
  <%= render "rui/shared/admin/nav", { header_title: "Vehicles" } do %>
    <%= render "rui/shared/admin/create_dropdown" %>
  <% end %>
<% end%>

<p style="color: green"><%= notice %></p>

<% content_for :title, "Vehicles" %>

<h1>Vehicles</h1>

<div id="vehicles">
  <% @vehicles.each do |vehicle| %>
    <%= render vehicle %>
    <p>
      <%= link_to "Show this vehicle", vehicle %>
    </p>
  <% end %>
</div>

<%= link_to "New vehicle", new_vehicle_path %>
