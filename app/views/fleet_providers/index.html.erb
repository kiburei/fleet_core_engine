<% content_for :header do %>
  <%= render "rui/shared/admin/nav", { header_title: "Fleet Providers" } do %>
    <%= render "rui/shared/admin/create_dropdown" %>
  <% end %>
<% end%>

<p style="color: green"><%= notice %></p>

<% content_for :title, "Fleet providers" %>

<h1>Fleet providers</h1>

<div id="fleet_providers">
  <% @fleet_providers.each do |fleet_provider| %>
    <%= render fleet_provider %>
    <p>
      <%= link_to "Show this fleet provider", fleet_provider %>
    </p>
  <% end %>
</div>

<%= link_to "New fleet provider", new_fleet_provider_path %>
