<% content_for :header do %>
  <%= render "rui/shared/admin/nav", { header_title: "Vehicle Model" } do %>
  <div class="flex flex-wrap gap-4 mt-6">
    <%= link_to "Edit", edit_vehicle_model_path(@vehicle_model), class: "btn btn-secondary" %>
    <%= link_to "Back", vehicle_models_path, class: "btn btn-primary" %>
    <%= button_to "Destroy", @vehicle_model, method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-danger" %>
  </div>
  <% end %>
<% end%>

<p style="color: green"><%= notice %></p>

<div class="pt-4 px-4 sm:px-6 lg:px-8">
  <%= render @vehicle_model %>
</div>
