<% content_for :header do %>
  <%= render "rui/shared/admin/nav", { header_title: "Trips" } do %>
    <%= render "rui/shared/admin/create_dropdown" %>
  <% end %>
<% end%>

<p style="color: green"><%= notice %></p>

<% content_for :title, "Trips" %>

<h1>Trips</h1>

<div id="trips">
  <% @trips.each do |trip| %>
    <%= render trip %>
    <p>
      <%= link_to "Show this trip", trip %>
    </p>
  <% end %>
</div>

<%= link_to "New trip", new_trip_path %>
